{
  "name": "try-react",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "dependencies": {
    "@emotion/core": "10.0.28",
    "normalize.css": "8.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.0",
    "@testing-library/react": "10.4.3",
    "@testing-library/user-event": "12.0.11",
    "@types/jest": "24.9.1",
    "@types/node": "14.0.14",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-jest": "23.17.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-only-error": "1.0.2",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-sort-keys-fix": "1.1.1",
    "eslint-plugin-standard": "4.0.1",
    "import-sort-style-module": "6.0.0",
    "npm-check-updates": "7.0.1",
    "prettier": "2.0.5",
    "prettier-plugin-import-sort": "0.0.4",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "build": "react-scripts build",
    "eslint-print-config": "eslint --print-config src/index.js > misc/eslint-print-config.json",
    "lint-dry": "eslint -c .eslintrc.js --ext .js,.jsx src",
    "lint": "eslint -c .eslintrc.js --fix --ext .js,.jsx src",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "update-all": "npm-check-updates -u"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "importSort": {
    ".js, .jsx": {
      "style": "module"
    }
  }
}
